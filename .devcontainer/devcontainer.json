{
    "name": "Laravel DevContainer",
    "forwardPorts": [
        8000
    ],
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "USERNAME": "dev",
            "USER_UID": "1000",
            "USER_GID": "1000"
        }
    },
    "workspaceFolder": "/workspace",
    "mounts": [
        "source=${localWorkspaceFolder},target=/workspace,type=bind"
    ],
    "remoteUser": "dev"
}